public class variables{
  public static void main(String[] args){

      int num = 23456;
      int ones = num % 10;
      System.out.println("Ones place digit:"+ones);
      num = num /10;
      int tens = num % 10
      System.out.println("Tens place digit:"+tens)
      num = num / 10;
      int hundreds = num % 10;
      System.out.println("Hundreds place digit:"+hundreds)
      num = num / 10;
      int thousands = num % 10;
      System.out.println("Thousands place digit:"+thousands)
      num = num / 10;
      int ten_thousands = num % 10;
     System.out.println("Ten thousands place digit:"+ten_thousands)
}
}
