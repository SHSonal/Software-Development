public class variables{
  public static void main(String[] args){

      int num = 23456;
      int ones = num % 10;
      System.out.println("Ones place digit:"+ones);
      num = num /10;
      int tens = num % 10;
      System.out.println("Tens place digit:"+tens;
      num = num / 10;
      int hundreds = num % 10;
      System.out.println("Hundreds place digit:"+hundreds);
      num = num / 10;
      int thousands = num % 10;
      System.out.println("Thousands place digit:"+thousands);
      num = num / 10;
      int ten_thousands = num % 10;
     System.out.println("Ten thousands place digit:"+ten_thousands);

        //OR

    //Create new scanner
    Scanner input = new Scanner(System.in);

    //Values of each digit
    int tenThousands, thousands, hundreds, tens, ones;

    //Prompt user to input 5 digit number
       
    int number = input.nextInt();
    if (number > 99999) {
        System.out.println("\nError! Number more than 5 digits.");
        }
    else if (number < 10000) {
        System.out.println("Error! Number less than 5 digits.");
        }
    else if (10000<=number&&number<=99999){
     tenThousands = number/10000;
      System.out.println(tenThousands);
      thousands = number/1000;
      thousands = thousands % 10;
      System.out.println(thousands);
      hundreds = number/100;
      hundreds= hundreds % 10;
      System.out.println(hundreds);
      tens = number/10;
      tens = tens % 10;
      System.out.println(tens);
      ones = number % 10;
      System.out.println(ones);

      
}

}
}
